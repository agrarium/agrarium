{
  "name": "@agrarium/cli",
  "description": "Agrarium CLI Toolbox",
  "version": "0.0.1",
  "author": "awinogradov <winogradovaa@gmail.com>",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "agrarium": "./bin/run"
  },
  "homepage": "https://github.com/agrarium/agrarium",
  "keywords": [],
  "bugs": "https://github.com/agrarium/agrarium/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agrarium/agrarium.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tsc -p . --noEmit && tslint -p . -t stylish",
    "prepublishOnly": "npm run build",
    "test": "mocha"
  },
  "dependencies": {
    "@agrarium/core": "*",
    "@agrarium/plugin": "*",
    "@oclif/command": "^1.4.2",
    "@oclif/config": "^1.3.59",
    "@oclif/errors": "^1.0.2",
    "@oclif/plugin-help": "^1.1.6",
    "@types/agrarium": "*",
    "@types/merge2": "^1.1.4",
    "@types/node": "^9.6.0",
    "@types/stream-to-array": "^2.3.0",
    "@types/through": "0.0.29",
    "fs-extra": "^5.0.0",
    "merge2": "^1.2.1",
    "mississippi": "^3.0.0",
    "stream-to-array": "^2.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "agrarium",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
